<?xml version="1.0" encoding="utf-8"?>

<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:macCollectionView="clr-namespace:MacCollectionView"
             x:Class="MacCollectionView.TestObjectView"
             BackgroundColor="White">
    <Grid x:DataType="macCollectionView:TestObject" WidthRequest="300">
        <Border Padding="5">
            <VerticalStackLayout Spacing="5">
                <Label TextColor="Black" Text="{Binding Double1}"/>
                <Label TextColor="Black" Text="{Binding Double2}"/>
                <Label TextColor="Black" Text="{Binding Double3}"/>
                <Label TextColor="Black" Text="{Binding Double4}"/>
                <Label TextColor="Black" Text="{Binding Double5}"/>
            </VerticalStackLayout>
        </Border>
    </Grid>
</ContentView>